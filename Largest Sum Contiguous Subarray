#include <iostream>
#include<climits>
using namespace std;

void Contiguous(int a[],int n)
{
    int i,j,sum;
    sum=INT_MIN;
    int maxsum_here=0;
    
    for(i=0;i<n;i++)
    {
        maxsum_here=maxsum_here+a[i];
        if(maxsum_here>sum)
          sum=maxsum_here;
        if(maxsum_here<=0)
          maxsum_here=0;
        
    }
    cout<<sum;
}
int main() 
{
	//code
   int n,i,j,k;
   cin>>n;
   int a[n];
   for(i=0;i<n;i++)
    cin>>a[i];
   Contiguous(a,n);
   
	return 0;
}
